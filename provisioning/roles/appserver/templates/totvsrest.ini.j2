[{{environment}}]
SourcePath={{rpo_path}}
RootPath={{dictionary_path}}
StartPath=\system\
LocalFiles={{local_files}}
Localdbextension={{localdbextension}}
PictFormat=Default
StringCodePage=2
RpoDb={{rpodb}}
