; Environment
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064764
[{{environment}}]
SourcePath={{rpo_path}}
RootPath={{dictionary_path}}
StartPath=\system\
LocalFiles={{local_files}}
Localdbextension={{localdbextension}}
PictFormat=Default
StringCodePage=2
RpoDb={{rpodb}}
RpoLanguage={{rpolanguage}}
RpoVersion={{rpoversion}}
DebugThreadUsedMemory={{DebugThreadUsedMemory}}
ThreadMemWarning={{ThreadMemWarning}}
ThreadMemStep={{ThreadMemStep}}
KillStack={{KillStack}}
Trace={{Trace}}
DBMemoMega={{DBMemoMega}}
TraceStack={{TraceStack}}
MaxLocks={{MaxLocks}}
AdvSQLReplay={{AdvSQLReplay}}
AdvSQLReplayLog={{AdvSQLReplayLog}}
AdvSQLReplayPath={{\SQLReplay\}}
AdvSQLReplayIOP={{AdvSQLReplayIOP}}
AdvSQLReplayLogMaxSize={{AdvSQLReplayLogMaxSize}}
LogProfilerPath={{LogProfilerPath}}

;DBAccess
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064885
;[DBAccess]
;Server= {{DBAccessServer}}
;Port={{dbaccess_slave_port}}
;Database={{database}}
;Alias={{Alias}}
;Driver={{Driver}}
;MemoMega={{MemoMega}}

;Drivers
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064839
[Drivers]
Active=TCP
MultiProtocolPort={{MultiProtocolPort}}
MultiProtocolPortSecure={{MultiProtocolPortSecure}}
Secure={{Secure}}

;TCP
;https://tdn.totvs.com/pages/viewpage.action?pageId=135495723
[TCP]
Type={{Type}}
Port={{rest_tcp_port}}
Ip={{Ip}}

;LicenseClient
;https://tdn.totvs.com/pages/viewpage.action?pageId=244734923
[LicenseClient]
Server={{license_ip}}
Port={{license_server_port}}

;LockServer
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064850
[LockServer]
Enable={{lockserverEnable}}
Server={{lockserver_ip}}
Port={{lockserver_port}}
SecureConnection={{lockserverSecureConnection}}
MessageLog={{lockserverMessageLog}}

;General
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064796
[General]
ChangeEncodingBehavior={{ChangeEncodingBehavior}}
InactiveTimeout={{InactiveTimeout}}
;app_environment=environment
CanAcceptDebugger={{CanAcceptDebugger}}
CanAcceptLB={{CanAcceptLB}}
CanAcceptMonitor={{CanAcceptMonitor}}
CanRunJobs={{CanRunJobs}}
CanAcceptRPC={{CanAcceptRPC}}
DebugThreadUsedMemory={{DebugThreadUsedMemory}}
FloatingPointPrecise={{FloatingPointPrecise}}
IdeMessages={{IdeMessages}}
ConsoleFile={{rest_consoleFile}}
AsyncConsoleLog=0
AsyncConsoleLog=0
LogTimeStamp = {{LogTimeStamp}}
ShowFullLog={{ShowFullLog}}
ShowIPClient={{ShowIPClient}}
SocketMessages={{SocketMessages}}
SSLRedirect={{SSLRedirect}}
LatencyLog={{LatencyLog}}
LatencyLogFile={{rest_latencyLogFile}}
LogHttpFuncs= {{LogHttpFuncs}}
MaxStringSize={{MaxStringSize}}
WsdlStreamType={{WsdlStreamType}}
SocketsDefaultIPV6={{SocketsDefaultIPV6}}
ServerMemoryLimit={{ServerMemoryLimit}}

;Service
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064864
[Service]
Name={{rest_name}}
Displayname={{rest_displayname}}
Description={{rest_description}}

;TDS
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064865
[TDS]
ALLOWAPPLYPATCH={{ALLOWAPPLYPATCH}}
ALLOWEDIT={{ALLOWEDIT}}
ENABLEDISCONNECTUSER={{ENABLEDISCONNECTUSER}}
ENABLESENDMESSAGE={{ENABLESENDMESSAGE}}
ENABLEBLOCKNEWCONNECTION={{ENABLEBLOCKNEWCONNECTION}}
ENABLESTOPSERVER={{ENABLESTOPSERVER}}

;APP_MONITOR
;https://tdn.totvs.com/pages/viewpage.action?pageId=552580433
[APP_MONITOR]
Enable={{enable_monitor}}

;update
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064898
[update]
Enable='{{ enable_update }}'
PathLinux={{update}}

;SSLConfigure
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064865
[SSLConfigure]
Verbose = 0
SSL2    = 0
SSL3    = 0
TLS1_0  = 1
TLS1_1  = 1
TLS1_2  = 1
Bugs    = 1
State   = 0
Debug   = 0

;SSL
;[SSL]
;TYPE=TCPIP
;PORT=26011
;SecureConnection=1

;WEBAPP
;https://tdn.totvs.com/pages/viewpage.action?pageId=307834830
[WEBAPP]
Port={{soap_http_port}}
EnvServer={{EnvServer}}
LastMainProg={{LastMainProg}}
MaxBodySize={{MaxBodySize}}
MaxHeaderSize={{MaxHeaderSize}}
MaxRequestTime={{MaxRequestTime}}


[WEBAPP-HEADERS]
Access-Control-Allow-Origin=*

[HTTPV11]
Enable=1
Sockets=HTTPREST

[HTTPREST]
Port={{rest_port}}
IPsBind=
URIs=HTTPURI
Security=1

[HTTPURI]
URL={{rest_url}}
PrepareIn={{rest_prepare_in}}
Instances={{rest_instances}}
CORSEnable=1
AllowOrigin=*

[HTTPJOB]
MAIN=HTTP_START
ENVIRONMENT={{environment}}

[ONSTART]
jobs=HTTPJOB
RefreshRate=30