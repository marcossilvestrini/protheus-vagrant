;TDS
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064865
[TDS]
ALLOWAPPLYPATCH={{ALLOWAPPLYPATCH}}
ALLOWEDIT={{ALLOWEDIT}}
ENABLEDISCONNECTUSER={{ENABLEDISCONNECTUSER}}
ENABLESENDMESSAGE={{ENABLESENDMESSAGE}}
ENABLEBLOCKNEWCONNECTION={{ENABLEBLOCKNEWCONNECTION}}
ENABLESTOPSERVER={{ENABLESTOPSERVER}}

;APP_MONITOR
;https://tdn.totvs.com/pages/viewpage.action?pageId=552580433
[APP_MONITOR]
Enable={{Enable}}

;update
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064898
[update]
Enable=1
PathLinux={{update}}

;HTTP
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064817
[HTTP]
Enable=0
Port=8090
Compression=1
CorsEnable=1
instances=5,20,3,5
LogRequest=0
LogResponse=0
ENABLEHTTPPROT=1
TIMEMSHTTPPROT=100
Path=D:\ERP\Html
ip=172.16.84.213

;HTTPS
;https://tdn.totvs.com/pages/viewpage.action?pageId=135496188
;[HTTPS]
;ENABLE=1
;PORT=443
;PATH=C:\AP_DATA\HTTPS
;ip=172.16.84.213
;ClientCertVerify=0
;SecureCookie=1

;SSLConfigure
;https://tdn.totvs.com/pages/viewpage.action?pageId=6064865
[SSLConfigure]
Verbose = 0
SSL2    = 0
SSL3    = 0
TLS1_0  = 1
TLS1_1  = 1
TLS1_2  = 1
Bugs    = 1
State   = 0
Debug   = 0

;SSL
;[SSL]
;TYPE=TCPIP
;PORT=26011
;SecureConnection=1

;WEBAPP
;https://tdn.totvs.com/pages/viewpage.action?pageId=307834830
[WEBAPP]
Port=8080
EnvServer=ENVIRONMENT,MSSQLP11,MSSQLP12,MSSQLTST
LastMainProg=SIGAADV,SIGAMDI,SIGACFG,SRVWIZARD
MaxBodySize=10485760
MaxHeaderSize=1024
MaxRequestTime=30

[WEBAPP-HEADERS]
Access-Control-Allow-Origin=*

;ONSTART
;[ONSTART]
;Jobs=JOBTEST
;RefreshRate=180
;DebugMsg=1

;[JOBTEST]
;Main=RecalcJur()
;Environment=EnvSQL
